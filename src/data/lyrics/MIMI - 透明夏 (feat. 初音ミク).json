    {
        "time": 43474,
        "text": "昼前の旅の今日の片隅でさ",
        "cn": "于上午之旅的今日的一角"
    },
    {
        "time": 46471,
        "text": "描いた地図に沿って歩く",
        "cn": "沿着描绘出的地图步行着"
    },
    {
        "time": 50000,
        "text": "",
        "interlude": true
    },
    {
        "time": 51082,
        "text": "舞う軽い羽に残る夏の音は",
        "cn": "于飘舞着的轻轻羽毛残存下的夏之音"
    },
    {
        "time": 55007,
        "text": "ずっと胸の奥に隠してまた繋ぐの",
        "cn": "一直隐藏在内心深处并再次联系着"
    },
    {
        "time": 58969,
        "text": "",
        "interlude": true
    },
    {
        "time": 60640,
        "text": "覆う風の色にふわり染まると",
        "cn": "被充满风的颜色轻轻地染上"
    },
    {
        "time": 63598,
        "text": "変わる今に唄いたくて",
        "cn": "想在改变了的如今歌唱"
    },
    {
        "time": 67550,
        "text": "",
        "interlude": true
    },
    {
        "time": 68128,
        "text": "写真の中で遠く笑うように",
        "cn": "为了像照片中那般远远地笑着"
    },
    {
        "time": 72047,
        "text": "そっと独り笛をとって目を閉じてみた",
        "cn": "悄悄地独自拿着笛子试着闭上眼睛"
    },
    {
        "time": 76442,
        "text": "",
        "interlude": true
    },
    {
        "time": 94727,
        "text": "潤む透明夏 願う今をさ",
        "cn": "朦胧的透明夏日 祈愿着的如今"
    },
    {
        "time": 99000,
        "text": "無数に浮かべては",
        "cn": "无数地浮现出来"
    },
    {
        "time": 101176,
        "text": "",
        "interlude": true
    },
    {
        "time": 102230,
        "text": "一人風に吹かれて想う明日(あす)に",
        "cn": "独自吹着风想要向期待的明日"
    },
    {
        "time": 106478,
        "text": "少し寄り掛かりたくて",
        "cn": "稍微靠近"
    },
    {
        "time": 110109,
        "text": "",
        "interlude": true
    },
    {
        "time": 111822,
        "text": "滲む四分間 揺れる僕の背",
        "cn": "渗入的四分钟 我那摇晃着的背"
    },
    {
        "time": 116034,
        "text": "ふわり包み込んだ",
        "cn": "轻轻的包围起来"
    },
    {
        "time": 117774,
        "text": "",
        "interlude": true
    },
    {
        "time": 119288,
        "text": "風に揺られてさ掠れて遊ぶ",
        "cn": "被风儿吹动着似游戏般掠过"
    },
    {
        "time": 123532,
        "text": "平凡な今日を乗せて",
        "cn": "乘上平凡的今日"
    },
    {
        "time": 126539,
        "text": "",
        "interlude": true
    },
    {
        "time": 127805,
        "text": "また・・・",
        "cn": "再次..."
    },
    {
        "time": 128172,
        "text": "",
        "interlude": true
    },
    {
        "time": 128548,
        "text": "なんでもない振りをして",
        "cn": "装作无所谓的样子"
    },
    {
        "time": 130435,
        "text": "言葉をすり減らした午後",
        "cn": "被语言所消磨的午后"
    },
    {
        "time": 132443,
        "text": "どうして大人はこうも真面目なのかな",
        "cn": "为何大人们总是如此认真呢"
    },
    {
        "time": 136327,
        "text": "",
        "interlude": true
    },
    {
        "time": 136948,
        "text": "なんて思うのも退屈な",
        "cn": "什么的如此想着也甚是无趣"
    },
    {
        "time": 139014,
        "text": "くらいに世界は広いからさ",
        "cn": "那般因为世界很是广阔所以"
    },
    {
        "time": 141274,
        "text": "まあ今は無理に焦らなくてもいいよな",
        "cn": "嘛今天不勉强着焦急也没关系的吧"
    },
    {
        "time": 144749,
        "text": "",
        "interlude": true
    },
    {
        "time": 145980,
        "text": "変わる景色も",
        "cn": "不断变换的景色也好"
    },
    {
        "time": 148213,
        "text": "霞む未来も",
        "cn": "朦胧不清的未来也罢"
    },
    {
        "time": 150189,
        "text": "案外意味なんて無くて",
        "cn": "意外地毫无意义"
    },
    {
        "time": 152870,
        "text": "",
        "interlude": true
    },
    {
        "time": 153417,
        "text": "世界が暮れてく・・・",
